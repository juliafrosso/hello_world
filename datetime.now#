from datetime import datetime
now = datetime.now()


print '%s/%s/%s %s:%s:%s' % (now.month, now.day, now.year, now.hour, now.minute, now.second)

# print '%s' (something)
# o uso de %s faz com que se imprima a variável contida nos parenteses, na ordem estalebecida
# a forma que se separa os %s também pode ser alterada, pode-se usar / ou : ou qualquer outra divisão (inclusive nada)

